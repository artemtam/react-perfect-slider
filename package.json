{
  "name": "react-perfect-slider",
  "version": "1.1.0",
  "description": "High-efficient and perfectly designed carousel/slider component.",
  "main": "dist/react-perfect-slider.umd.js",
  "module": "dist/react-perfect-slider.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "microbundle --jsx React.createElement --external react ./src/index.tsx --output dist",
    "start": "microbundle --jsx React.createElement --external react ./src/index.tsx --output dist --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemtam/react-perfect-slider.git"
  },
  "keywords": [
    "carousel",
    "slider",
    "react"
  ],
  "author": "Artem Tamoian <hello@artemtam.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artemtam/react-perfect-slider/issues"
  },
  "homepage": "https://github.com/artemtam/react-perfect-slider#readme",
  "dependencies": {},
  "peerDependencies": {
    "react": ">=^16.8",
    "react-dom": ">=^16.8"
  },
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "microbundle": "^0.13.0",
    "release-it": "^14.3.0"
  }
}
